#include <iostream>
#include <ctime>
using namespace std;

int main()
{
	int a;
	srand(time(NULL));

	char string[] = 
{ 
'a','b','c','d','e','f','g'
,'h','i','j','k','l','m','n'
,'o','p','q','r','s','t','u'
,'v','w','x','y','z','\0' 
};

	for (int i = 0; i < strlen(string); i++) {

		a = rand() % strlen(string);
		//cout << string[a] << endl;

			if (a == 0) {

				cout << string[a] << " glasnaya" << endl;

			}
			if (a == 4) {

				cout << string[a] << " glasnaya" << endl;

			}
			if (a == 8) {

				cout << string[a] << " glasnaya" << endl;

			}
			if (a == 14) {

				cout << string[a] << " glasnaya" << endl;

			}
			if (a == 20) {

				cout << string[a] << " glasnaya" << endl;

			}
			else
